{
  "name": "coge",
  "version": "0.1.0",
  "description": "A efficient code generator.",
  "license": "MIT",
  "repository": "https://gitr.net/ty/coge",
  "author": "",
  "keywords": [
    "aos",
    "aost"
  ],
  "files": [
    "bin",
    "lib",
    "templates"
  ],
  "bin": {
    "coge": "bin/coge.js"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\" --single-quote --trailing-comma es5",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "npm run test:code && npm run test:require",
    "test:win32": "jest --runInBand metaverse",
    "test:code": "jest",
    "test:metaverse": "jest metaverse",
    "test:require": "node lib/cli.timed.js init self; rm -rf _templates"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "commander": "^5.0.0",
    "ejs": "^3.0.2",
    "enquirer": "^2.3.5",
    "execa": "^1.0.0",
    "front-matter": "^3.1.0",
    "fs-extra": "^9.0.0",
    "ignore-walk": "^3.0.3",
    "inflection": "^1.12.0",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/ejs": "^3.0.2",
    "@types/fs-extra": "^8.1.0",
    "@types/inflection": "^1.5.28",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11",
    "dir-compare": "^2.2.0",
    "jest": "^25.3.0",
    "nyc": "^15.0.1",
    "pkg": "^4.4.7",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "time-require": "^0.1.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8",
    "typescript": "^3.8"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "src/**/*",
    "outPath": "standalone",
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  }
}
