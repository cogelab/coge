{
  "name": "coge",
  "version": "0.8.0",
  "description": "An efficient code generator.",
  "license": "MIT",
  "repository": "https://github.com/cogelab/coge",
  "author": "",
  "keywords": [
    "coge",
    "cli",
    "development",
    "dev",
    "build",
    "tool",
    "scaffold",
    "stack",
    "app",
    "generate",
    "generator",
    "generation",
    "template",
    "boilerplate",
    "redux",
    "react"
  ],
  "files": [
    "bin",
    "lib",
    "generators"
  ],
  "bin": {
    "co": "bin/coge",
    "coge": "bin/coge"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "release": "np --no-2fa",
    "standalone": "rimraf standalone; npm run build && pkg package.json --out-path standalone && npm run standalone:postprocess",
    "standalone:postprocess": "node scripts/standalone.js",
    "standalone:brew-sha": "shasum -a 256 standalone/*.macos.*.tar.gz",
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\" --single-quote --trailing-comma es5",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "npm run clean && npm run build && npm run test:code && npm run test:require",
    "test:win32": "jest --runInBand metaverse",
    "test:code": "jest",
    "test:metaverse": "jest metaverse",
    "test:require": "mkdirp temp; cd temp; node ../lib/cli.timed.js init; cd ..; rm -rf temp"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@loopx/utils": "^2.5.0",
    "caporal": "^1.4.0",
    "chalk": "^4.1.0",
    "change-case": "^4.1.2",
    "coge-environment": "^0.8.0",
    "coge-generator": "^0.4.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.1",
    "diff": "^5.0.0",
    "ejs": "^3.1.5",
    "execa": "^5.0.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "ignore-walk": "^3.0.3",
    "inflection": "^1.12.0",
    "inquirer": "^7.3.3",
    "is-scoped": "^2.1.0",
    "json5": "^2.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.5",
    "@types/fs-extra": "^9.0.6",
    "@types/inflection": "^1.5.28",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.19",
    "@types/json5": "^0.0.30",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^14.14.19",
    "dir-compare": "^2.4.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "micromatch": "^4.0.2",
    "np": "^7.2.0",
    "pkg": "^4.4.9",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "time-require": "^0.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "pkg": {
    "scripts": "lib/**/*.js",
    "assets": "src/**/*",
    "outPath": "standalone",
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  }
}
