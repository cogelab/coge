{
  "name": "coge",
  "version": "0.1.9",
  "description": "An efficient code generator.",
  "license": "MIT",
  "repository": "https://github.com/cogelab/coge",
  "author": "",
  "keywords": [
    "coge",
    "cli",
    "development",
    "dev",
    "build",
    "tool",
    "scaffold",
    "stack",
    "app",
    "generate",
    "generator",
    "generation",
    "template",
    "boilerplate",
    "redux",
    "react"
  ],
  "files": [
    "bin",
    "lib",
    "templates"
  ],
  "bin": {
    "coge": "bin/coge"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "standalone": "rimraf standalone; npm run build && pkg package.json --out-path standalone && npm run standalone:postprocess",
    "standalone:postprocess": "node scripts/standalone.js",
    "standalone:brew-sha": "shasum -a 256 standalone/*.macos.*.tar.gz",
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\" --single-quote --trailing-comma es5",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "npm run clean && npm run build && npm run test:code && npm run test:require",
    "test:win32": "jest --runInBand metaverse",
    "test:code": "jest",
    "test:metaverse": "jest metaverse",
    "test:require": "mkdirp temp; cd temp; node ../lib/cli.timed.js init; cd ..; rm -rf temp"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@tiopkg/utils": "^2.1.8",
    "caporal": "^1.3.0",
    "change-case": "^4.1.1",
    "coge-environment": "^0.3.2",
    "cross-spawn": "^7.0.2",
    "debug": "^4.1.1",
    "diff": "^4.0.2",
    "ejs": "^3.1.2",
    "execa": "^4.0.0",
    "front-matter": "^3.1.0",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "ignore-walk": "^3.0.3",
    "inflection": "^1.12.0",
    "inquirer": "^7.1.0",
    "is-scoped": "^2.1.0",
    "json5": "^2.1.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.2",
    "@types/fs-extra": "^8.1.0",
    "@types/inflection": "^1.5.28",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/json5": "0.0.30",
    "@types/node": "^13.13",
    "dir-compare": "^2.3.0",
    "jest": "^25.4.0",
    "pkg": "^4.4.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "time-require": "^0.1.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9",
    "typescript": "^3.8"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "pkg": {
    "scripts": "lib/**/*.js",
    "assets": "src/**/*",
    "outPath": "standalone",
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  }
}
